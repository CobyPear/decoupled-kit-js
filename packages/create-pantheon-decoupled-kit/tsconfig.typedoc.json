{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"types": ["vitest/globals"],
		"jsx": "react-jsx",
		"baseUrl": "./",
		"paths": {
			"@cli/*": ["./src/*"],
			"@partials/*": ["./src/templates/partials/*"]
		}
	},
	"exclude": [
		"node_modules",
		"dist",
		"./src/templates/*",
		"__tests__",
		"__mocks__"
	]
}
